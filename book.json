{
    "title": "记录分享",
    "author": "mingo",
    "language": "zh-hans",
    "links": {
      "sidebar": {
        "GitHub": "https://github.com/zhangmy163/notes"
      }
    },
    "plugins": [
      "-livereload",
      "image-captions",
      "github",
      "page-treeview@2.9.8",
      "editlink"
    ],
    "pluginsConfig": {
      "image-captions": {
        "attributes": {
          "width": "600"
        },
        "caption": "图 _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
      },
      "github": {
        "url": "https://github.com/zhangmy163/notes"
      },
      "editlink": {
        "base": "https://github.com/zhangmy163/notesblob/master/",
        "label": "编辑本页"
      },
      "page-treeview": {
        "copyright": "Copyright &#169; mingo",
        "minHeaderCount": "2",
        "minHeaderDeep": "2"
      }
    }
  }