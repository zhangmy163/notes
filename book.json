      },
      "editlink": {
        "base": "https://github.com/zhangmy163/notes/blob/master/",
        "label": "编辑本页"
      },
      "anchor-navigation-ex": {
        "showLevel": false
      },
      "donate": {
        "alipay": "./source/images/donate.png",
        "title": "",
        "button": "赞赏",
        "alipayText": " "
      }
    
    }
